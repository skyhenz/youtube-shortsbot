{
    "version": 2,
    "builds": [
        {
            "src": "dashboard/server.js",
            "use": "@vercel/node"
        },
        {
            "src": "dashboard/public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/dashboard/server.js"
        },
        {
            "src": "/(.*)",
            "dest": "/dashboard/public/$1"
        }
    ]
}